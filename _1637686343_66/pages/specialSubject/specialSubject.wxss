@import "../../__wuBaseWxss__/0.wxss";

@import "../../__wuBaseWxss__/9.wxss";

@import "../../__wuBaseWxss__/5.wxss";

@import "../../__wuBaseWxss__/10.wxss";

.m-vote-box {
    padding: 36rpx 30rpx 0;
}

.m-vote-title {
    font-size: 30rpx;
    color: rgba(255,255,255,0.7);
    line-height: 48rpx;
    height: 48rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    font-weight: 700;
}

.m-vote-subtitle {
    font-size: 24rpx;
    color: rgba(255,255,255,0.3);
    line-height: 42rpx;
}

.m-vote-list {
    padding: 27rpx 0 30rpx;
}

.m-vote-item {
    height: 90rpx;
    border-radius: 45rpx;
    border: 2rpx solid rgba(206,12,2,0.4);
    box-sizing: border-box;
    margin-top: 20rpx;
    color: #ce0c02;
    font-size: 30rpx;
    line-height: 30rpx;
    padding: 30rpx 0;
    text-align: center;
    background: #0f0000;
    position: relative;
    overflow: hidden;
}

.m-vote-item:first-child,.m-vote-item:last-child {
    margin-top: 10rpx;
}

.m-vote-btn {
    height: 90rpx;
    border-radius: 45rpx;
    color: #ce0c02;
    font-size: 30rpx;
    background: transparent;
    position: absolute;
    width: 100%;
    top: 0rpx;
}

.c-vote-star {
    position: absolute;
    left: 0;
    right: 0;
    z-index: 3;
    line-height: 90rpx;
    font-weight: 700;
}

.m-voteOK .c-vote-star {
    color: rgba(255,255,255,0.7);
    transform: translateY(-50%);
}

.c-vote-num {
    font-size: 26rpx;
    color: rgba(255,255,255,0.7);
    position: absolute;
    right: 40rpx;
    opacity: 0;
    line-height: 26rpx;
    top: 32rpx;
}

.c-vote-line {
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
    background: rgba(165,7,1,0.35);
    opacity: 0;
}

.vote-share-tip {
    font-size: 28rpx;
    color: #ce0c02;
    line-height: 30rpx;
    text-align: center;
    display: none;
}

.vote-share-tip .vote-share-btn {
    display: inline-block;
    margin: 0;
    padding: 30rpx 0 10rpx;
    border-radius: 0;
    font-size: 28rpx;
    line-height: 30rpx;
    background-color: transparent;
    color: #ce0c02;
}

.m-vote-list.m-voteOK .vote-share-tip {
    display: block;
}

.m-vote-list.m-voteOK .m-vote-item {
    color: rgba(255,255,255,0.7);
}

.m-vote-list.m-voteOK .c-vote-num,.m-vote-list.m-voteOK .c-vote-line {
    opacity: 1;
}

.m-vote-list.m-voteOK .m-vote-item.selected .c-vote-line {
    background: #a50701;
    opacity: 1;
}

@import "../../__wuBaseWxss__/8.wxss";

@import "../../__wuBaseWxss__/2.wxss";

.m-scrollList-paopaofeed {
    height: 590rpx;
}

.m-scrollList-paopaofeed-inner {
    display: flex;
    height: 100%;
}

.m-paopao-feed-item {
    padding-left: 30rpx;
    box-sizing: content-box;
    flex: none;
}

.m-paopao-feed-item:last-child {
    padding-right: 30rpx;
}

.m-paopao-feedBox {
    width: 550rpx;
    height: 590rpx;
    border-radius: 12rpx;
    position: relative;
}

.m-paopao-feedBox-bg {
    width: 100%;
    height: 100%;
}

.m-paopao-feedBox-content {
    width: 100%;
    height: 100%;
    padding: 30rpx 30rpx 40rpx;
    box-sizing: border-box;
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
}

.m-feed-article {
    font-size: 28rpx;
    color: rgba(255,255,255,0.7);
    margin-top: 18rpx;
    line-height: 42rpx;
    min-height: 42rpx;
    max-height: 168rpx;
    display: -webkit-box;
    word-break: break-all;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 4;
    overflow: hidden;
    text-overflow: ellipsis;
}

.m-feed-user {
    position: relative;
    display: box;
    height: 68rpx;
}

.m-feed-userImg {
    position: relative;
    width: 68rpx;
    height: 68rpx;
    margin-right: 20rpx;
}

.c-feed-userImg {
    width: 100%;
    height: 100%;
    border-radius: 50%;
    overflow: hidden;
}

.m-feed-userInfo {
    position: relative;
    box-flex: 1;
}

.c-feed-userName {
    height: 42rpx;
    line-height: 42rpx;
    font-size: 24rpx;
    color: rgba(255,255,255,0.7);
}

.c-feed-time {
    height: 26rpx;
    line-height: 26rpx;
    font-size: 20rpx;
    color: rgba(255,255,255,0.3);
}

.c-feed-text {
    margin-left: 14rpx;
}

.c-feed-text:first-child {
    margin-left: 0;
}

.m-imgFeed-more {
    display: flex;
    justify-content: flex-start;
    flex-wrap: wrap;
    margin-top: 20rpx;
    margin-left: -2rpx;
    overflow: hidden;
}

.m-imgFeed-more-item {
    width: 50%;
    padding-left: 2rpx;
    box-sizing: border-box;
}

.m-imgFeed-more-inner {
    position: relative;
    width: 100%;
}

.m-imgFeed-more-inner::before {
    display: block;
    content: "";
    width: 100%;
    padding-top: 100%;
}

.m-imgFeed-more-img {
    position: absolute;
    top: 0;
    right: 0;
    left: 0;
    bottom: 0;
    width: 100%;
    height: 100%;
}

@import "../../__wuBaseWxss__/13.wxss";

