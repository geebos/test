page {
    height: 100%;
}

.container-special {
    background-image: url(http://statics-web.iqiyi.com/h5/wechat/images/specialSubject-bgimg-repeat.png);
    background-repeat: repeat-y;
    background-size: 100% auto;
    height: auto;
    background-color: #000;
}

.container-special.container-overflow-hidden {
    height: 100%;
    overflow: hidden;
}

.container-special .m-box,.container-special .m-box-top {
    border-color: rgba(255,255,255,0.1);
}

.m-myPoints {
    height: 80rpx;
    display: flex;
    font-size: 24rpx;
    line-height: 80rpx;
    padding: 0 30rpx;
    width: 100%;
    box-sizing: border-box;
    color: #fff;
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
}

.m-myPoints-left {
    text-align: left;
    flex: 1;
}

.points-btn {
    color: #ce0c02;
    display: inline-block;
    line-height: 80rpx;
    padding: 0;
    margin: 0 0 0 18rpx;
    border-radius: 0;
    background-color: transparent;
    vertical-align: top;
    font-size: 24rpx;
}

.myPoints-num {
    text-decoration: underline;
}

.m-myPoints-right {
    text-align: right;
}

.specialSubject-header {
    width: 100%;
    height: 472rpx;
}

.m-play.m-play-active {
    width: 690rpx;
    height: 388rpx;
    margin: -92rpx auto 0;
}

.m-play.m-play-active .video {
