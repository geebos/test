.m-play {
    position: relative;
    width: 100%;
    height: 422rpx;
}

.video {
    width: 100%;
    height: 422rpx;
}

.m-videopic {
    position: absolute;
    top: 0;
    left: 0;
    width: 750rpx;
    height: 422rpx;
    background-color: rgb(153,153,153);
}

.m-videopic_pic {
    width: 100%;
    height: 100%;
}

.m-videopic_info {
    width: 100%;
    height: 99rpx;
    position: absolute;
    bottom: 0;
    left: 0;
}

.m-videopic_num {
    font-size: 24rpx;
    position: absolute;
    bottom: 0;
    left: 0;
    color: #fff;
    padding-left: 24rpx;
    padding-bottom: 24rpx;
    z-index: 5;
}

.m-videopic_time {
    font-size: 24rpx;
    position: absolute;
    bottom: 0;
    right: 0;
    z-index: 5;
    padding-right: 24rpx;
    padding-bottom: 24rpx;
    color: #fff;
}

.m-video-text-shadow {
    text-shadow: 0px 0px 4rpx rgba(0,0,0,0.7);
}

.m-advertisement {
    width: 100%;
    font-size: 28rpx;
    padding: 46rpx 30rpx 46rpx 30rpx;
    box-sizing: border-box;
    border: 1rpx solid rgb(230,230,230);
}

.c-nummber {
    color: rgb(11,190,6);
}