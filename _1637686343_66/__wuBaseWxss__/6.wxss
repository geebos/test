.m-from-search-before {
    width: 100%;
    height: 86rpx;
}

.m-from-search-before-box {
    padding: 20rpx;
    box-sizing: border-box;
    position: fixed;
    top: 0;
    width: 100%;
    background-color: #fff;
    z-index: 10;
}

.m-from-search-before-content {
    height: 66rpx;
    background-color: #f0f0f0;
    border-radius: 66rpx;
    box-sizing: border-box;
    position: relative;
    text-align: center;
}

.search-before-input {
    height: 66rpx;
    font-size: 28rpx;
    min-height: 30rpx;
    border-radius: 30rpx;
    color: #999;
    line-height: 66rpx;
    display: inline-block;
}

.c-icon-search-center {
    z-index: 5;
    display: inline-block;
    width: 30rpx;
    height: 30rpx;
    margin-right: 13rpx;
    vertical-align: middle;
}

.m-from-search {
    width: 100%;
    height: 108rpx;
}

.m-from-search-box {
    position: fixed;
    top: 0;
    width: 100%;
    padding: 20rpx;
    box-sizing: border-box;
    display: flex;
    background-color: #fff;
    border-bottom: 1px solid #f0f0f0;
    z-index: 10;
}

.m-from-search-content {
    height: 66rpx;
    background-color: #f0f0f0;
    border-radius: 66rpx;
    box-sizing: border-box;
    position: relative;
    flex: 1;
    padding-left: 74rpx;
    padding-right: 56rpx;
    line-height: 66rpx;
    min-height: 66rpx;
    padding-top: 0;
}

.search-input {
    height: 66rpx;
    font-size: 28rpx;
    line-height: 66rpx;
    min-height: 66rpx;
    margin-top: 0;
    padding-top: 0;
}

.c-icon-search-layer {
    position: absolute;
    z-index: 5;
    display: block;
    width: 30rpx;
    height: 30rpx;
    left: 30rpx;
    top: 50%;
    transform: translateY(-50%);
}

.c-icon-close {
    position: absolute;
    z-index: 5;
    display: block;
    width: 36rpx;
    height: 36rpx;
    right: 20rpx;
    top: 50%;
    transform: translateY(-50%);
}

.m-from-search-text {
    font-size: 28rpx;
    color: #333;
    line-height: 66rpx;
    padding: 0 13rpx 0 33rpx;
}

.m-from-user-contact {
    position: relative;
}

.m-from-user-contact_btn {
    text-align: left;
    background-color: #fff;
    width: 100%;
    height: 120rpx;
    box-sizing: border-box;
    padding: 45rpx 20rpx;
    display: flex;
    justify-content: space-between;
    font-size: 30rpx;
    line-height: 30rpx;
    color: #333;
    border: none;
    font-weight: 700;
}

.m-from-user-contact_btn::after {
    border: none;
}

.m-from-user-contact_link {
    position: absolute;
    top: 35rpx;
    right: 10px;
}

.m-from-search-home-content {
    flex: 1;
    height: 100%;
    background-color: rgba(255,255,255,0.7);
    border-radius: 56rpx;
    box-sizing: border-box;
    position: relative;
    text-align: center;
}

.search-home-input {
    height: 100%;
    min-height: 30rpx;
    border-radius: 30rpx;
    display: inline-block;
}

.m-from-search-home {
    height: 56rpx;
    flex: 1;
    line-height: 56rpx;
    font-size: 26rpx;
    display: initial;
    color: #333;
}

.m-home_search_form {
    height: 100%;
    flex: 1;
    margin-top: 16rpx;
    display: initial;
}

.m-iqySearch_input {
    height: 84rpx;
    line-height: 84rpx;
    border-radius: 84rpx;
    background-color: #fff;
    margin: 0 30rpx 46rpx;
    font-size: 30rpx;
    color: #999;
}