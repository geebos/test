.m-bottomCard-tips-box {
    width: 100%;
    height: 80rpx;
}

.m-bottomCard-tips-green {
    position: fixed;
    bottom: 0;
    right: 0;
    left: 0;
    z-index: 50;
    width: 100%;
    height: 80rpx;
    box-sizing: border-box;
    padding: 0 20rpx;
    line-height: 80rpx;
    color: #fff;
    font-size: 24rpx;
    background-color: #0aab06;
    display: flex;
    transition: bottom .8s cubic-bezier(0.165,0.84,0.44,1);
}

.bottomCard-tips-left {
    flex: 1;
}

.m-bottomCard-circlebox {
    position: fixed;
    bottom: 140rpx;
    right: 20rpx;
    z-index: 100;
    opacity: 0.8;
}

.m-sideBar-item {
    font-size: 18rpx;
    color: #452202;
    padding: 14rpx 0;
    width: 88rpx;
    height: 88rpx;
    box-sizing: border-box;
    text-align: center;
    margin-top: 20rpx;
    border-radius: 50%;
    background: linear-gradient(to right,#edb97c,#d7964f);
    background-repeat: repeat-x;
    font-weight: 700;
}

.m-sideBar-item .vote-share-btn {
    font-size: 18rpx;
    color: #452202;
    padding: 0;
    margin: 0;
    width: 88rpx;
    height: 88rpx;
    border-radius: 0;
    background-color: transparent;
    line-height: 1;
}

.m-sideBar-item .vote-share-btn:after {
    border: none;
}

.c-icon-sideBar-img {
    width: 38rpx;
    height: 38rpx;
    display: block;
    margin: 0 auto 4rpx;
}

.m-bottomCard-video {
    position: fixed;
    bottom: 188rpx;
    right: 20rpx;
    z-index: 100;
}

.m-bottomCard-video .m-sideBar-item {
    color: #fff;
    background: rgba(0,0,0,0.5);
}